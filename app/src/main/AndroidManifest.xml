<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adam.rotationalschedule">

    <!-- [START gcm_permission] -->
    <!-- <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" /> -->
    <!-- [END gcm_permission] -->

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2_web"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".scheduleDay"
            android:label="@string/title_activity_schedule_day"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".scheduleMonth"
            android:label="@string/title_activity_schedule_month"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".popup"
            android:label="@string/title_activity_popup"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

    </application>


</manifest>
